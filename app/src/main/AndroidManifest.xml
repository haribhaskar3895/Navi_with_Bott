<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.naviwithbott">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainSuggestion" />
        <activity android:name=".MainSubscribtion" />
        <activity android:name=".MainSearch" />
        <activity android:name=".MainMore" />
        <activity android:name=".MainInviteUser" />
        <activity android:name=".MainEditProfile" />
        <activity android:name=".MainCustomerSupport" />
        <activity android:name=".MainAnnouncement" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>